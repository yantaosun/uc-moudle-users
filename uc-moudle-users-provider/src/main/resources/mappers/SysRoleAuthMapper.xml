<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.uc.moudle.user.provider.mappers.SysRoleAuthMapper">    <resultMap id="BaseResultMap" type="com.ucs.moudlecommon.domain.SysRoleAuth">        <id column="id" property="id" jdbcType="INTEGER"/>        <result column="role_code" property="roleCode" jdbcType="VARCHAR"/>        <result column="role_auth" property="roleAuth" jdbcType="VARCHAR"/>        <result column="role_auth_desc" property="roleAuthDesc" jdbcType="VARCHAR"/>    </resultMap>   <select id="findUserRoles" resultType="java.lang.String" parameterType="com.ucs.moudlecommon.params.sysuser.Find_UserInfoArg">SELECT auths.role_auth FROM sys_role_auth auths                 INNER JOIN sys_roles  role                 INNER JOIN sys_user_role ur                 INNER JOIN sys_users users                ON auths.role_code = role.role_code                AND ur.role_code = role.role_code                AND ur.user_code = users.id                AND                <if test="arg.memPhoneNumber != null">                    users.mem_phone_number = #{arg.memPhoneNumber}                </if>                <if test="arg.memGithubUnionId != null">                    users.mem_github_union_id = #{arg.memGithubUnionId}                </if>                <if test="arg.memWxUnionId != null">                    users.mem_wx_union_id = #{arg.memWxUnionId}                </if>                <if test="arg.memWbUnionId != null">                    users.mem_wb_union_id = #{arg.memWbUnionId}                </if>                <if test="arg.memQqUnionId != null">                    users.mem_qq_union_id = #{arg.memQqUnionId}                </if>   </select></mapper>